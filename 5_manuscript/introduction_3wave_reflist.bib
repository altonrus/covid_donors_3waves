	author = {Besag, Julian and York, Jeremy and Mollié, Annie},
	month = mar,
	year = {1991},
	keywords = {archeology, Bayesian restoration, epidemiology, Gibbs sampler, image analysis, spatial statistics},
	pages = {1--20},

	pages = {S17--S22},
	file = {JSTOR Full Text PDF:C\:\\Users\\yyu\\Zotero\\storage\\JG95I6J7\\Pampalon et al. - 2012 - An Area-based Material and Social Deprivation Inde.pdf:application/pdf},
}


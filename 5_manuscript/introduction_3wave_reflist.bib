	year = {2001},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1046/j.1423-0410.2001.00035.x},
	keywords = {blood donor, blood product, enterovirus, PCR, viraemia},
	pages = {211--215},

@article{custer_surveillance_2023,
	title = {Surveillance for {Zika}, {Chikungunya}, and {Dengue} {Virus} {Incidence} and {RNAemia} in {Blood} {Donors} at 4 {Brazilian} {Blood} {Centers} {During} 2016–2019},
	volume = {227},
	issn = {0022-1899},
	url = {https://doi.org/10.1093/infdis/jiac173},
	doi = {10.1093/infdis/jiac173},
	abstract = {Except for public health case reports, the incidence of Zika virus (ZIKV), chikungunya virus (CHIKV), and dengue virus (DENV) infection are not available to assess the potential blood transfusion safety threat in Brazil.Pools of 6 donation samples (MP6) left over from human immunodeficiency virus, hepatitis B virus, and hepatitis C virus nucleic acid testing were combined to create MP18 pools (3 MP6 pools). Samples were tested using the Grifols triplex ZIKV, CHIKV, and DENV real-time transcription mediated amplification assay to estimate prevalence of RNAemia and incidence, and to compare these results to case reports in São Paulo, Belo Horizonte, Recife, and Rio de Janeiro, from April 2016 through June 2019.ZIKV, CHIKV, and DENV RNAemia were found from donors who donated without overt symptoms of infection that would have led to deferral. The highest RNAemic donation prevalence was 1.2\% (95\% CI, .8\%–1.9\%) for DENV in Belo Horizonte in May 2019. Arbovirus infections varied by location and time of year, and were not always aligned with annual arbovirus outbreak seasons in different regions of the country.Testing donations for arboviruses in Brazil can contribute to public health. Transfusion recipients were likely exposed to ZIKV, CHIKV, and DENV viremic blood components during the study period.},
	number = {5},
	urldate = {2023-05-14},
	journal = {The Journal of Infectious Diseases},
	author = {Custer, Brian and Grebe, Eduard and Buccheri, Renata and Bakkour, Sonia and Stone, Mars and Capuani, Ligia and Alencar, Cecilia and Amorim, Luiz and Loureiro, Paula and Carneiro-Proietti, Anna Barbara and Mendrone-Junior, Alfredo and Gonçalez, Thelma and Gao, Kui and Livezey, Kristin W and Linnen, Jeffrey M and Brambilla, Don and McClure, Chris and Busch, Michael P and Sabino, Ester C and {for the Recipient Epidemiology and Donor Evaluation Study (REDS-III) International Component Brazil}},
